<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="ModeloPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>co.com.beanslab.cashretriver.modelo.Cobros</class>
    <class>co.com.beanslab.cashretriver.modelo.Roles</class>
    <class>co.com.beanslab.cashretriver.modelo.Deudas</class>
    <class>co.com.beanslab.cashretriver.modelo.Personas</class>
    <class>co.com.beanslab.cashretriver.modelo.Municipios</class>
    <class>co.com.beanslab.cashretriver.modelo.Salidas</class>
    <class>co.com.beanslab.cashretriver.modelo.Departamentos</class>
    <class>co.com.beanslab.cashretriver.modelo.Barrios</class>
    <class>co.com.beanslab.cashretriver.modelo.Usuarios</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/cashretriver?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="preist"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
